        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/tac"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/cid"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:textColor="#584718"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/plmn"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/mnc"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/mcc"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:textSize="16sp" />
