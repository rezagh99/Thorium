<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="false"
    android:layout_marginBottom="5dp"
    app:cardElevation="10dp"
    app:cardCornerRadius="15dp"
    android:layout_margin="10dp"
    app:cardBackgroundColor="@color/d6bbe4"
    app:cardMaxElevation="10dp"
    app:cardPreventCornerOverlap="true"
    app:cardUseCompatPadding="true"
    android:background="@drawable/item_background"
    android:divider="@drawable/border"
    android:orientation="vertical"
    android:showDividers="middle">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="10dp">

    <TextView
        android:id="@+id/type"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="@string/type"
        android:textColor="#584718"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/gsm_rssi"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/gsm_rssi"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/strength"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/strength"
        android:textSize="16sp" />


    <TextView
        android:id="@+id/time"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#584718"
        android:fontFamily="sans-serif-medium"
        android:text="@string/time"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/altitude"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/altitude"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/longitude"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/longitude"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/arfcn"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/arfcn"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/lac"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/lac"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/tac"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/tac"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/cid"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:textColor="#584718"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="@string/cell_identity"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/plmn"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/plmn"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/mnc"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/mnc"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/mcc"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="@string/mcc"
        android:textSize="16sp" />
    <TextView
        android:id="@+id/latency"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#584718"
        android:fontFamily="sans-serif-medium"
        android:text="Latency: "
        android:textSize="16sp" />

    <TextView
        android:id="@+id/content_latency"
        style="@style/word_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:textColor="#584718"
        android:text="Content Latency: "
        android:textSize="16sp" />


    </LinearLayout>
</androidx.cardview.widget.CardView>